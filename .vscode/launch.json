  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug API Server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/packages/server/src/dev.ts",
      "outFiles": [
        "${workspaceFolder}/packages/server/dist/**/*.js"
      ],
      "envFile": "${workspaceFolder}/.env",
      "preLaunchTask": "Build Server",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Judger",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/packages/judger/src/dev.ts",
      "outFiles": [
        "${workspaceFolder}/packages/judger/dist/**/*.js"
      ],
      "envFile": "${workspaceFolder}/.env",
      "preLaunchTask": "Build Judger",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Libraries",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/packages/libraries/src/index.ts",
      "outFiles": [
        "${workspaceFolder}/packages/libraries/dist/**/*.js"
      ],
      "envFile": "${workspaceFolder}/.env",
      "preLaunchTask": "Build Shared Libraries",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Upload Server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/packages/upload/src/dev.ts",
      "outFiles": [
        "${workspaceFolder}/packages/upload/dist/**/*.js"
      ],
      "envFile": "${workspaceFolder}/.env",
      "preLaunchTask": "Build Upload Server",
      "outputCapture": "std"
    }
  ]
}
